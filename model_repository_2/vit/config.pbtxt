name: "vit"
platform: "ensemble"
max_batch_size: 4
input [
  {
    name: "INPUT_0"
    data_type: TYPE_UINT8
    dims: [ -1 ]
  }
]
output [
  {
    name: "probabilities"
    data_type: TYPE_FP32
    dims: [ 2 ]
  },
  {
    name: "predicted_class"
    data_type: TYPE_INT64
    dims: [ 1 ]
  }
]
ensemble_scheduling {
  step [
    {
      model_name: "vit_preprocessing"
      model_version: -1
      input_map {
        key: "INPUT_0"
        value: "INPUT_0"
      }
      output_map {
        key: "OUTPUT_0"
        value: "preprocessed_image"
      }
    },
    {
      model_name: "vit_model_onnx"
      model_version: -1
      input_map {
        key: "input"
        value: "preprocessed_image"
      }
      output_map {
        key: "output"
        value: "logits"
      }
    },
    {
      model_name: "vit_postprocessing"
      model_version: -1
      input_map {
        key: "INPUT_0"
        value: "logits"
      }
      output_map {
        key: "probabilities"
        value: "probabilities"
      }
      output_map {
        key: "predicted_class"
        value: "predicted_class"
      }
    }
  ]
}
